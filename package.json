{
  "name": "gh-api-consume",
  "version": "1.0.0",
  "description": "Github API consume.",
  "scripts": {
    "start": "ntl -e start",
    "dev:test": "concurrently -p \"[{name}]\" -n \"PARCEL,JEST\" -c \"bgBlue.bold,bgMagenta.bold\" \"parcel src/index.html --port 3000\" \"npm run test:watch\"",
    "development": "npm run clean && parcel src/index.html --port 3000",
    "build": "npm run clean && parcel build src/index.html --public-url https://devastion.net/gh-api",
    "clean": "rimraf ./dist && rimraf ./.parcel-cache",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "ntl": {
    "descriptions": {
      "dev:test": "dev",
      "development": "Development environment",
      "build": "Production ready build",
      "clean": "Clears cache and development directory"
    }
  },
  "author": "Dimitar Banev <banev_dimitar@mail.com> (https://devastion.net/)",
  "license": "MIT",
  "devDependencies": {
    "@parcel/transformer-typescript-tsc": "^2.5.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.1.1",
    "@testing-library/user-event": "^14.1.1",
    "@types/jest": "^27.4.1",
    "@types/react": "^18.0.5",
    "@types/react-dom": "^18.0.1",
    "@types/react-redux": "^7.1.24",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "concurrently": "^7.1.0",
    "eslint": "^8.13.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "jest": "^27.5.1",
    "ntl": "^5.1.0",
    "parcel": "^2.5.0",
    "prettier": "^2.6.2",
    "process": "^0.11.10",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.6.2",
    "@mui/material": "^5.6.2",
    "@reduxjs/toolkit": "^1.8.1",
    "axios": "^0.26.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-redux": "^8.0.1",
    "react-router-dom": "^6.3.0"
  }
}
